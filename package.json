{
  "name": "figma-plugin-template",
  "version": "1.0.0",
  "description": "Figma Plugin Template",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "yarn watch",
    "dev:ui": "vite -c ./vite.config.ui.ts",
    "watch": "yarn watch:ui & yarn watch:plugin",
    "watch:ui": "yarn build:ui --watch --mode=development",
    "watch:plugin": "yarn build:plugin --watch --mode=development",
    "build": "yarn types && yarn clean && yarn build:ui && yarn build:plugin",
    "build:ui": "vite build -c ./vite.config.ui.ts",
    "build:plugin": "vite build -c ./vite.config.plugin.ts",
    "types": "yarn tsc",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@figma/plugin-typings": "^1.121.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "prettier": "^3.7.1",
    "sass": "^1.94.2",
    "typescript": "^5.3.2",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.2.4",
    "vite-plugin-singlefile": "^2.3.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
